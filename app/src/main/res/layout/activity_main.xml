                android:id="@+id/oyoID"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:layout_margin="4dp"
                android:background="@drawable/oyo"/>
            <Button
                android:id="@+id/courseeraID"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:layout_margin="4dp"
                android:background="@drawable/courseera"/>
            <Button
                android:id="@+id/hackerrankID"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:layout_margin="4dp"
                android:background="@drawable/hackerrank"/>
            <Button
                android:id="@+id/linkedinButtonID"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:layout_margin="4dp"
                android:background="@drawable/linkedin"/>
        </LinearLayout>
</LinearLayout>
